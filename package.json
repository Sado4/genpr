{
  "name": "genpr",
  "displayName": "PR Prompt Generator",
  "description": "Generate PR descriptions from git commit diffs and templates.",
  "version": "0.0.4",
  "publisher": "Sado4",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other",
    "SCM Providers"
  ],
  "icon": "images/icon.png",
  "keywords": [
    "git",
    "pull request",
    "pr template",
    "prompt",
    "ai",
    "chatgpt",
    "clipboard"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "genpr.generatePrompt",
        "title": "Generate PR Prompt from Commit Diff"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sado4/genpr"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.98.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
